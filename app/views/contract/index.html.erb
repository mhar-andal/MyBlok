<h1 class="header">Data Retrieval</h1>


<form id="data-retrieval" class="form header" action="/users" method="post">
   <fieldset>
    <legend>Open Data Locker</legend>

    <p class="personalID">
        <input type="text" name="locker[personalID]" id="personalID" required>
        <label for="bank_name">Personal ID</label>
    </p>

    <p class="LockerID">
        <input type="text" name="locker[lockerID]" id="LockerID" required>
        <label for="bank_name">Locker Number</label>
    </p>


    <p class="submit">
        <input id="save" type="submit" value="Save">
    </p>

   </fieldset>
</form>
